<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.culfest.nit.jamshedpur.Developers">

    <!-- TODO: Update blank fragment layout -->
    <android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:id="@+id/rishav"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        card_view:cardBackgroundColor="#fff"
        card_view:cardElevation="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="5dip">

            <LinearLayout
                android:id="@+id/thumbnail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginRight="5dip"
                android:padding="3dip">

                <ImageView
                    android:id="@+id/photot"
                    android:layout_width="60dip"
                    android:layout_height="60dip"
                    android:background="@drawable/rishav" />
            </LinearLayout>

            <TextView
                android:id="@+id/tvt1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@id/thumbnail"
                android:layout_marginLeft="15dp"
                android:layout_toRightOf="@id/thumbnail"
                android:text="Rishav Sagar"
                android:textColor="#000"
                android:textSize="25dip"
                android:typeface="sans" />

            <TextView
                android:id="@+id/tvt2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvt1"
                android:layout_marginLeft="15dp"
                android:layout_toRightOf="@id/thumbnail"
                android:text="Computer Science and Engineering"
                android:textColor="#000"
                android:textSize="15dip"
                android:typeface="sans" />

            <TextView
                android:id="@+id/tvt3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvt2"
                android:layout_marginLeft="15dp"
                android:layout_toRightOf="@id/thumbnail"
                android:paddingBottom="10dip"
                android:text="NIT Jamshedpur"
                android:textColor="#000"
                android:textSize="18dip"
                android:typeface="sans" />

        </RelativeLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:id="@+id/mayank"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        card_view:cardBackgroundColor="#fff"
        card_view:cardElevation="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="5dip">

            <LinearLayout
                android:id="@+id/thumbnail_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginRight="5dip"
                android:padding="3dip">

                <ImageView
                    android:layout_width="60dip"
                    android:layout_height="60dip"
                    android:background="@drawable/mayank" />
            </LinearLayout>

            <TextView
                android:id="@+id/tvt4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@id/thumbnail_1"
                android:layout_marginLeft="15dp"
                android:layout_toRightOf="@id/thumbnail_1"
                android:text="Mayank Agarwal"
                android:textColor="#000"
                android:textSize="25dip"
                android:typeface="sans" />

            <TextView
                android:id="@+id/tvt5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvt4"
                android:layout_marginLeft="15dp"
                android:layout_toRightOf="@id/thumbnail_1"
                android:text="Computer Science and Engineering"
                android:textColor="#000"
                android:textSize="15dip"
                android:typeface="sans" />

            <TextView
                android:id="@+id/tvt6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvt5"
                android:layout_marginLeft="15dp"
                android:layout_toRightOf="@id/thumbnail_1"
                android:paddingBottom="10dip"
                android:text="NIT Jamshedpur"
                android:textColor="#000"
                android:textSize="18dip"
                android:typeface="sans" />

        </RelativeLayout>

    </android.support.v7.widget.CardView>


    <android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:id="@+id/kodi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        card_view:cardBackgroundColor="#fff"
        card_view:cardElevation="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="5dip">

            <LinearLayout
                android:id="@+id/thumbnail_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginRight="5dip"
                android:padding="3dip">

                <ImageView
                    android:layout_width="60dip"
                    android:layout_height="60dip"
                    android:background="@drawable/kodi" />
            </LinearLayout>

            <TextView
                android:id="@+id/tvt7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@id/thumbnail_2"
                android:layout_marginLeft="15dp"
                android:layout_toRightOf="@id/thumbnail_2"
                android:text="Praveen Kumar"
                android:textColor="#000"
                android:textSize="25dip"
                android:typeface="sans" />

            <TextView
                android:id="@+id/tvt8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvt7"
                android:layout_marginLeft="15dp"
                android:layout_toRightOf="@id/thumbnail_2"
                android:text="Computer Science and Engineering"
                android:textColor="#000"
                android:textSize="15dip"
                android:typeface="sans" />

            <TextView
                android:id="@+id/tvt9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvt8"
                android:layout_marginLeft="15dp"
                android:layout_toRightOf="@id/thumbnail_2"
                android:paddingBottom="10dip"
                android:text="NIT Jamshedpur"
                android:textColor="#000"
                android:textSize="18dip"
                android:typeface="sans" />

        </RelativeLayout>

    </android.support.v7.widget.CardView>


</LinearLayout>
